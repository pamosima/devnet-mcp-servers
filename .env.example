# Meraki MCP Server Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

# Required: Your Meraki Dashboard API Key
MERAKI_KEY=your_actual_meraki_api_key_here

# Optional: Meraki API base URL (defaults to official API)
MERAKI_BASE_URL=https://api.meraki.com/api/v1

# Optional: Role for access control (defaults to 'noc')
# Values: noc | sysadmin | all
MCP_ROLE=noc

# Optional: Enable debug logging
DEBUG=false
